class College
{
	String name;
	String course;
	int code;
	
	College(int code,String name,String Course){
		this.code=code;
		this.name=name;
		this.course=course;
	}
	
	void getcode(){
		System.out.println("The college code is : "+code);
	}
	void getname(){
		System.out.println("The College name is : "+name);
	}
	void getcourse(){
		System.out.println("The course in College is : "+course);
	}
}

class Faculty extends College
{
	int faculty_code;
	String faculty_name;
	String subject;
	
	Faculty(int faculty_code,String faculty_name,String subject,String name,String course,int code){
		super(code,name,course);
		this.faculty_code=faculty_code;
		this.faculty_name=faculty_name;
		this.subject=subject;
	}
	
	void display(){
		super.getcode();
		super.getcourse();
		super.getname();
		System.out.println("Faculty Code: "+faculty_code);
		System.out.println("Faculty Name: "+faculty_name);
		System.out.println("Faculty Subject: "+subject);
		}
	}
	
	class VisitingFaculty extends Faculty
	{
		int no_of_hrs,per_hr_rate,total_payment;
		VisitingFaculty(int faculty_code,String faculty_name,String subject,String name,String course,int code,int no_of_hrs,int per_hr_rate,int total_payment){
			super(faculty_code,faculty_name,subject,name,course,code);
			this.no_of_hrs=no_of_hrs;
			this.per_hr_rate=per_hr_rate;
			this.total_payment=total_payment;
		}
		void display(){
		super.display();
		System.out.println("No of hrs Temp Faculty worked: "+no_of_hrs);
		System.out.println("Per hour rate of Temp Faculty : "+per_hr_rate);
		System.out.println("Total Payment of Temp Faculty : "+total_payment);
		}
}

class PermenantFaculty extends Faculty
	{
		String designation;
		int salary;
		PermenantFaculty(int faculty_code,String faculty_name,String subject,String name,String course,int code,String designation,int salary){
			super(faculty_code,faculty_name,subject,name,course,code);
			this.designation=designation;
			this.salary=salary;
		}
		void dispay(){
			super.display();
			System.out.println("Permenant Facult's Designantion: "+designation);
			System.out.println("Permenant Facult's Salary: "+salary);
			
		}
	}

class Student extends College
{
	int rollno;
	String Stud_name;
	
	Student(int rollno,String Stud_name,String course,String name,int code){
		super(code,name,course);
		this.rollno=rollno;
		this.Stud_name=Stud_name;
	}
	
	void display(){
		System.out.println("Student RollNo: "+rollno);
		System.out.println("Student Name: "+Stud_name);
		System.out.println("Student Course: "+course);
	}
	
	
}



