abstract class Employee
{
	abstract void transportation();
	abstract void meal();
}

class Fulltime extends Employee
{
	int salary;
	Fulltime(int salary){
		this.salary=salary;
	}
	void transportation(){
		int trans=1500;
		salary=salary-trans;
	}
	void meal(){
		int meal=1500;
		salary=salary-meal;
	}
	void display_salary(){
		System.out.println("Total Salary=" + salary);
	}
}

class PartTime extends Employee
{
	int h;
	int salary;
	
	PartTime(int h){
		this.h=h;
	}
	
	{
		if(h==3)
			salary=500;
		else if(h<=5 && h>3)
			salary=1000;
		else if(h>=5)
			salary=1500;
		
	}
	
	void transportation(){
		salary=salary-100;
	}
	void meal(){
		salary=salary-150;
	}
	void display_salary(){
		System.out.println("Total Salary=" + salary);
	}
	
}


class  Emp
{
	public static void main(String[] args) 
	{
		//System.out.println("Hello World!");
		Fulltime ft=new Fulltime(60000);
		ft.transportation();
		ft.meal();
		ft.display_salary();
		
		PartTime pt=new PartTime(5);
		pt.transportation();
		pt.meal();
		pt.display_salary();
	}
}
