class lessbalance extends Exception
{
	lessbalance(String message){
		super(message);
	}
}



interface cstaccount
{
	int deposit(int newbal);
	int withdraw(int debit);
	void display();
	int interest=5;
}

class savingacc implements cstaccount{
	 private int balance =0;
	
	public int deposit(int newbal){
		balance=balance+newbal;
		return balance;
	}
	
	public int withdraw(int debit){
		try{
			if(debit < balance)
				balance=balance-debit;
			else
				throw new lessbalance("Your balance is less to credit");
		}
		
		catch (lessbalance e) {
			System.out.println(e));
		}
		return balance;
	}
	
	public void display(){
		System.out.println("Remeaning Balance: "+balance);
	}
}

