


class threadone implements Runnable
{
	public void run(){
		try{
			for (int i=0;i<=5 ;i++ )
			{
				System.out.println(Thread.currentThread().getname()+" : "+i);
				Thread.sleep(500);
			}
		}
		catch(InterruptedException e){
			System.out.println(e.getMessage());
		}
	}
}

class threadtwo implements Runnable
{
	public void run(){
		try{
			for (char i='a';i<'e' ;i++ )
			{
				System.out.println(Thread.currentThread().getName()+" : "+i);
				Thread.sleep(500);
			}
		}
		catch(InterruptedException e){
		System.out.println(e.getMessage());
		}
	}
}


