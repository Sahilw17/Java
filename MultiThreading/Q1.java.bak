class Q1AMultiTh extends Thread
{
	public void run(){
		try{
			for (int i=1;i<=100 ;i++ )
			{
				if(i % 8==0){
					System.out.println(Thread.CurrentThread().getName()+" Divisor of 8 :  "+i);
					Thread.sleep(500);
				}
			}
		}
		catch(InterruptedException e){
			System.out.println(e.getMessage());
		}
		
	}
}

