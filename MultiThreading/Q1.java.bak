class Q1BMultiTh extends Thread
{
	public void run(){
		try{
			for (int i=51;i<=100 ;i++ )
			{
				if(i % 2==0){
					System.out.println(Thread.CurrentThread().getName()+" Even Number :  "+i);
					Thread.sleep(500);
				}
			}
		}
		catch(InterruptedException e){
			System.out.println(e.getMessage());
		}
		
	}
}
