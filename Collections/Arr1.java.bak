//ArrayList

import java.util.*;

class  Arr1
{
	public static void main(String[] args) 
	{
		//System.out.println("Hello World!");
		ArrayList ar=new ArrayList();
		ar.add("Sahil");
		ar.add(100);
		ar.add(1.3);
		ar.add(2,17);
		System.out.println("The elements in arraylist are: "+ar);
		
		ar.remove("Sahil");
		ar.remove(1);
		
		ar.set(2,"Tierd");
		
		System.out.println("The elements after update in arraylist are: "+ar);

	
	}
}
